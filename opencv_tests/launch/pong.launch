<launch>
  <master auto="start"/>
  <env name="GMON_OUT_PREFIX" value="pong1" />
  <node name="pong1" pkg="image_view" type="image_view" args="image:=/opencv_tests/images" respawn="false" output="screen"
    launch-prefix="valgrind --tool=callgrind --callgrind-out-file='callgrind.pong1.%p' " />
  <env name="GMON_OUT_PREFIX" value="pong2" />
  <node name="pong2" pkg="opencv_tests" type="source.py" respawn="false"
    launch-prefix="valgrind --tool=callgrind --callgrind-out-file='callgrind.pong2.%p' " />
</launch>
